<resources>

    <style name="TabTextAppearance">
        <item name="textAllCaps">false</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textSize">@dimen/txt_body_small</item>
        <item name="android:fontFamily">@font/sourcesanspro_regular</item>
    </style>

    <style name="SubTabTextAppearance">
        <item name="textAllCaps">false</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textSize">@dimen/txt_13sp</item>
        <item name="android:fontFamily">@font/sourcesanspro_regular</item>
    </style>

    <style name="Bubble" parent="android:Widget.TextView">
        <item name="android:gravity">center</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:background">@color/color_red</item>
    </style>

    <style name="EtLanding">
        <item name="android:paddingTop">@dimen/vertical_padding</item>
        <item name="android:paddingBottom">@dimen/vertical_padding</item>
        <item name="android:paddingLeft">@dimen/horizontal_padding</item>
        <item name="android:paddingRight">@dimen/horizontal_padding</item>
        <item name="android:background">@drawable/bg_et_landing</item>
        <item name="android:textSize">@dimen/txt_body_normal</item>
        <item name="android:textColor">@color/color_text_primary</item>
        <item name="android:textColorHint">@color/color_text_secondary</item>
        <item name="android:drawablePadding">@dimen/horizontal_padding</item>
        <item name="android:lines">1</item>
        <item name="android:inputType">text</item>
    </style>

    <style name="EtLanding.PhoneNumber">
        <item name="android:paddingLeft">@dimen/dimen_5dp</item>
    </style>

    <style name="TvCountryCode">
        <item name="android:paddingTop">@dimen/vertical_padding</item>
        <item name="android:paddingBottom">@dimen/vertical_padding</item>
        <item name="android:paddingLeft">@dimen/horizontal_padding</item>
        <item name="android:background">@drawable/bg_et_landing</item>
        <item name="android:textSize">@dimen/txt_body_normal</item>
        <item name="android:textColor">@color/color_text_primary</item>
        <item name="android:textColorHint">@color/color_text_secondary</item>
        <item name="android:drawablePadding">@dimen/horizontal_padding</item>
        <item name="android:lines">1</item>
        <item name="android:inputType">text</item>
    </style>

    <style name="TvPreviewInfringement">
        <item name="android:paddingTop">@dimen/dimen_10dp</item>
        <item name="android:paddingBottom">@dimen/dimen_10dp</item>
        <item name="android:textSize">@dimen/txt_body_normal</item>
        <item name="android:textColor">@color/color_text_secondary</item>
        <item name="android:drawablePadding">@dimen/horizontal_padding</item>
        <item name="android:lines">1</item>
        <item name="android:inputType">text</item>
    </style>

    <style name="TvForm">
        <item name="android:paddingTop">@dimen/vertical_padding</item>
        <item name="android:paddingBottom">@dimen/vertical_padding</item>
        <item name="android:paddingLeft">@dimen/horizontal_padding</item>
        <item name="android:paddingRight">@dimen/horizontal_padding</item>
        <item name="android:background">@drawable/bg_et_landing</item>
        <item name="android:textSize">@dimen/txt_body_normal</item>
        <item name="android:textColor">@color/color_text_primary</item>
        <item name="android:textColorHint">@color/color_text_secondary</item>
        <item name="android:drawablePadding">@dimen/horizontal_padding</item>
        <item name="android:lines">1</item>
        <item name="android:ellipsize">end</item>
    </style>

    <style name="EtMyProfile">
        <item name="android:paddingTop">@dimen/vertical_padding</item>
        <item name="android:paddingBottom">@dimen/vertical_padding</item>
        <item name="android:paddingLeft">@dimen/horizontal_padding</item>
        <item name="android:paddingRight">@dimen/horizontal_padding</item>
        <item name="android:background">@drawable/bg_et_landing</item>
        <item name="android:textSize">@dimen/txt_body_normal</item>
        <item name="android:textColor">@color/color_text_primary</item>
        <item name="android:textColorHint">@color/color_text_secondary</item>
        <item name="android:drawablePadding">@dimen/horizontal_padding</item>
        <item name="android:lines">1</item>
        <item name="android:inputType">text</item>
    </style>

    <style name="EtMyProfile.Enabled">
        <item name="android:paddingLeft">@dimen/dimen_5dp</item>
        <item name="android:textColor">@color/color_text_editable</item>
        <item name="android:textColorHint">@color/color_text_un_editable_dark</item>
    </style>

    <style name="EtMyProfile.Disabled">
        <item name="android:textColor">@color/color_text_un_editable_dark</item>
        <item name="android:textColorHint">@color/color_text_un_editable_dark</item>
        <item name="android:enabled">false</item>
    </style>

    <style name="EtLanding.Password">
        <item name="android:inputType">textPassword</item>
        <item name="android:maxLength">18</item>
    </style>

    <style name="BtnWrap">
        <item name="android:minWidth">@dimen/dimen_200dp</item>
        <item name="android:paddingTop">@dimen/vertical_padding</item>
        <item name="android:paddingBottom">@dimen/vertical_padding</item>
        <item name="android:paddingStart">@dimen/vertical_margin</item>
        <item name="android:paddingEnd">@dimen/vertical_margin</item>
        <item name="android:background">@drawable/bg_button</item>
        <item name="android:textSize">@dimen/txt_body_normal</item>
        <item name="android:textColor">@color/color_white</item>
        <item name="android:gravity">center</item>
        <item name="android:lines">1</item>
        <item name="android:ellipsize">end</item>
    </style>

    <style name="BtnWrap.DisabledByDefault">
        <item name="android:enabled">false</item>
    </style>

    <style name="BtnWithBorder">
        <item name="android:minWidth">@dimen/dimen_200dp</item>
        <item name="android:paddingTop">@dimen/vertical_padding</item>
        <item name="android:paddingBottom">@dimen/vertical_padding</item>
        <item name="android:paddingLeft">@dimen/vertical_margin</item>
        <item name="android:paddingRight">@dimen/vertical_margin</item>
        <item name="android:background">@drawable/bg_btn_identify_vrf</item>
        <item name="android:textSize">@dimen/txt_body_normal</item>
        <item name="android:textColor">@color/color_btn_identity_vrf</item>
        <item name="android:gravity">center</item>
        <item name="android:lines">1</item>
        <item name="android:ellipsize">end</item>
        <item name="android:enabled">false</item>
        <item name="android:textAllCaps">false</item>
    </style>

    <style name="TvLblLanding">
        <item name="android:paddingTop">@dimen/dimen_5dp</item>
        <item name="android:paddingBottom">@dimen/dimen_5dp</item>
        <item name="android:textSize">@dimen/txt_body_normal</item>
        <item name="android:textColor">@color/color_text_primary</item>
        <item name="android:inputType">textCapSentences</item>
        <item name="android:lines">1</item>
    </style>

    <style name="TvLbl">
        <item name="android:paddingTop">@dimen/dimen_5dp</item>
        <item name="android:paddingBottom">@dimen/dimen_5dp</item>
        <item name="android:textSize">@dimen/txt_body_normal</item>
        <item name="android:inputType">textCapSentences</item>
        <item name="android:lines">1</item>
    </style>

    <style name="TvLbl.Disabled">
        <item name="android:textColor">@color/color_text_un_editable</item>
    </style>

    <style name="TvLbl.Enabled">
        <item name="android:textColor">@color/color_text_primary</item>
    </style>

    <style name="TvSettings">
        <item name="android:paddingTop">@dimen/vertical_padding</item>
        <item name="android:paddingBottom">@dimen/vertical_padding</item>
        <item name="android:paddingStart">@dimen/dimen_30dp</item>
        <item name="android:paddingEnd">@dimen/dimen_30dp</item>
        <item name="android:textSize">@dimen/txt_body_normal</item>
        <item name="android:textColor">@color/color_text_primary</item>
        <item name="android:inputType">textCapSentences</item>
        <item name="android:background">@drawable/shp_bottom_line</item>
        <item name="android:lines">1</item>
    </style>

    <style name="SpinnerStyle">
        <item name="android:minHeight">@dimen/dimen_50dp</item>
        <item name="android:background">@drawable/bg_et_landing</item>
    </style>

    <style name="TvNavDrawer">
        <item name="android:paddingStart">@dimen/dimen_30dp</item>
        <item name="android:paddingEnd">@dimen/dimen_30dp</item>
        <item name="android:paddingTop">@dimen/vertical_padding</item>
        <item name="android:paddingBottom">@dimen/vertical_padding</item>
        <item name="android:textSize">@dimen/txt_title</item>
        <item name="android:textColor">@color/color_text_primary</item>
        <item name="android:background">@drawable/sel_nav_item</item>
        <item name="android:lines">1</item>
        <item name="android:drawablePadding">@dimen/dimen_20dp</item>
    </style>
</resources>
